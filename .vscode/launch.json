  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/main.ts",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "trace": "verbose"
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch Program Local Config",
      "program": "${workspaceFolder}/src/main.ts",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "args": [
        "--config-file-path", "D:/projects/epoll/epoll-api.conf.json"
      ]
    }
  ]
}
